{
  "name": "es6-seed",
  "version": "1.0.0",
  "description": "seed project for es6 in the browser",
  "main": "index.js",
  "scripts": {
    "test": "karma start --reporters clear-screen,dots,coverage",
    "build": "node ./build.js",
    "serve": "browser-sync start --server \"public\" --files \"public/js/bundle.js\" --startPath \"index.html\""
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "author": "MFH",
  "license": "ISC",
  "dependencies": {
    "browserify": "13.1.1",
    "watchify": "3.11.1"
  },
  "devDependencies": {
    "babel-polyfill": "6.16.0",
    "babel-preset-es2015": "6.18.0",
    "babelify": "7.3.0",
    "standard": "14.3.1"
  },
  "standard": {
    "globals": [
      "describe",
      "beforeEach",
      "expect",
      "test",
      "afterEach",
      "jest",
      "beforeAll",
      "afterAll",
      "canvas",
      "window",
      "$"
    ]
  }
}
